<h1>This is you library</h1>
<h2><%- language.libname %></h2>
<form action="/profile/<%- id %>" method="POST">
   <label >Prompt:
    <input type="text" name="prompt">
   </label> 
    <button type="submit">Generate new Prompt </button>
</form>

<div class="all-prompts">
    <% prompts.forEach(prompt => { %>
    <div>
        <%- prompt.prompt %>
    </div>
    <div>
        <%- prompt.response %>
    </div>
    <form action="/profile/<%- id %>/<%- prompt._id %>/update" method="POST">
        <label> Update Prompt:
         <input type="text" name="prompt">
        </label> 
         <button type="submit">Update </button>
     </form>

    <form action="/profile/<%- id %>/<%- prompt._id %>/delete" method="POST">
        <button>Delete</button>
      </form>
    <% }) %>
</div>

